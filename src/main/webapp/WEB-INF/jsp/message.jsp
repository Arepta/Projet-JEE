<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!-- Page to display a message to the user -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Message Page</title>
</head>
<body>
    <!-- Display the message title -->
    <h1>${title}</h1>
    <!-- Display the message content -->
    <p>${message}</p>
</body>
</html>
